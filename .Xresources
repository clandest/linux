URxvt*geometry: 90x72
URxvt.depth: 32
URxvt.background: [90]#101010
URxvt.foreground: #f2f2f2
!URxvt.background: black
!URxvt.foreground: grey
URxvt.shading: 30
URxvt.scrollBar: false
URxvt.inheritPixmap: true
!URxvt.tintColor: red
URxvt.fading: 20
URxvt.saveLines: 3000
URxvt.termName: rxvt
URxvt.internalBorder: 0
URxvt.jumpScroll: true

urxvt*color0: #101010
urxvt*color1: #ffff00
urxvt*color2: #93f91d
urxvt*color3: #44ff0a
urxvt*color4: #004f9e
urxvt*color5: #ec0048
